        "####Suma y Promedio números"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "i-pHTiteyQXP"
      },
      "source": [
        "int s=0; // 1\n",
        "double prom=0; //1\n",
        "for (int i=0; i<n; i++)// 1+n(1+...+2)+1\n",
        "{\n",
        "    s=s+A[i]; // 3\n",
        "    prom=s/(double)n;//3\n",
        "}\n",
        "printf(\"Suma;%d\\nProm:%d\",s,prom);//2\n",
        "-------------------------------------------------\n",
        "//1+1+1+n(1+6+2)+1+2\n",
        "//3+n(9)+3\n",
        "//9n+6\n",
        "//Tiempo detallado: 9n+6\n",
        "//Tiempo asintotico: O(n)"
      ],
