        "##Ejercio 6\n",
        "Factorial"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "XdalwDPUTjf_"
      },
      "source": [
        "int fact = 1;//1\n",
        "for (int i = 2; i < n; i++) {//1+n(1+Interna+2)+1\n",
        "  fact *= i;//2\n",
        "}\n",
        "printf(\"Factorial: %d\", fact);//1\n",
        "//1+n(1+Interna+2)+4\n",
        "//1+n(1+2+2)+4\n",
        "//1+n(5)+4\n",
        "//5+5(n)"
      ],
      "execution_count": null,
